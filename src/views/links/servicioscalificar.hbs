<div class="container p-4">
    <div class="row">
        {{#each links}}
        <div class="col-md-3">
            <div class="card text-center">
                <div class="card-body">
                    <a href="" target="_blank">
                        <h3 class="card-title text-uppercase">
                            {{fecha}}
                        </h3>
                    </a>
                    <p class="m-2">{{direccion}}</p>
                    <p class="m-2">{{barrio}}</p>
                    <p class="m-2">{{nombre_aparato}}</p>
                    <p class="m-2">Tecnico: {{nombretec}} {{apetec}}</p>
                    <p class="m-2">Descrip: {{descripcion}}</p>
                    <p class="m-2">Precio: {{precio}}</p>
                    
                    <p class="m-2">Calificacion: {{calificacion}}</p>
                   
                   
                    <a class="btn btn-secondary" href="/links/calificar/{{idservicio}}">
                        Calificar servicio
                    </a>
                </div>
            </div>
        </div>
        {{else}}
        <div class="col-md-4 mx-auto">
            <div class="card card-body text-center">
                <p>No tienes servicios por calificar.</p>
                <a href="/links/add">Pide uno!</a>
            </div>
        </div>
        {{/each}}
    </div>
</div>